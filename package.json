{
  "name": "generator-ts-jest",
  "version": "1.2.0",
  "description": "Quick generate TypeScript project, testing with Jest",
  "keywords": [
    "yeoman-generator",
    "typescript",
    "jest"
  ],
  "main": "app/index.js",
  "types": "app/index.d.ts",
  "repository": "https://github.com/ikatyang/generator-ts-jest",
  "homepage": "https://github.com/ikatyang/generator-ts-jest#readme",
  "author": "ikatyang",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "tslint -p ./tsconfig.json --type-check",
    "prebuild": "rm -rf ./app",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp -r ./src/templates ./app",
    "test": "jest --config ./jest.json",
    "release": "standard-version"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "lodash.kebabcase": "^4.1.1",
    "yeoman-generator": "^2.0.0"
  },
  "devDependencies": {
    "@types/dedent": "0.7.0",
    "@types/jest": "20.0.8",
    "@types/lodash.kebabcase": "4.1.3",
    "@types/yeoman-generator": "1.0.3",
    "@types/yeoman-test": "1.7.1",
    "jest": "21.1.0",
    "prettier": "1.7.0",
    "prettier-config-ikatyang": "1.1.1",
    "standard-version": "4.2.0",
    "ts-jest": "21.0.1",
    "tslint": "5.7.0",
    "tslint-config-ikatyang": "2.4.0",
    "tslint-config-prettier-ext": "1.5.0",
    "tslint-plugin-prettier": "1.2.0",
    "typescript": "2.5.2",
    "yeoman-test": "1.7.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "/app/**/*"
  ]
}
