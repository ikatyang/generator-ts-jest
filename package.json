{
  "name": "generator-ts-jest",
  "version": "1.1.0",
  "description": "Quick generate TypeScript project, testing with Jest",
  "keywords": [
    "yeoman-generator",
    "typescript",
    "jest"
  ],
  "main": "app/index.js",
  "types": "app/index.d.ts",
  "repository": "https://github.com/ikatyang/generator-ts-jest",
  "homepage": "https://github.com/ikatyang/generator-ts-jest#readme",
  "author": "ikatyang",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "tslint -p ./tsconfig.json --type-check",
    "prebuild": "rm -rf ./app",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp -r ./src/templates ./app",
    "test": "jest --config ./jest.json",
    "format": "gulp format",
    "format-check": "gulp format-check"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "lodash.kebabcase": "^4.1.1",
    "yeoman-generator": "^1.1.1"
  },
  "devDependencies": {
    "@types/dedent": "0.7.0",
    "@types/gulp": "4.0.4",
    "@types/jest": "20.0.4",
    "@types/lodash.kebabcase": "4.1.2",
    "@types/prettier": "1.5.0",
    "@types/yeoman-generator": "1.0.2",
    "@types/yeoman-test": "1.7.0",
    "gulp": "3.9.1",
    "gulp-plugin-prettier": "1.0.0",
    "jest": "20.0.4",
    "prettier": "1.5.3",
    "ts-jest": "20.0.7",
    "ts-node": "3.2.1",
    "tslint": "5.5.0",
    "tslint-config-ikatyang": "1.0.1",
    "typescript": "2.4.2",
    "yeoman-test": "1.7.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "/app/**/*"
  ]
}
